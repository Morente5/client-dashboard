{
	"static.ignore": [
		"^\/assets\/.*$"
	],
	"external": {
		"urls": [
			{
				"url": "https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
			}
		]
	},
	"routing": {
		"index": "/index.html",
		"routes": {
			"/": {
				"prefix": false
			},
			"/tasks": {
				"prefix": false
			},
			"/social": {
				"prefix": false
			}
		}
	},
	"dynamic": {
		"group": [
			{
				"name": "something",
				"urls": {
					"/api/checkcachefirst": {
						"match": "prefix"
					}
				},
				"cache": {
					"optimizeFor": "performance",
					"strategy": "lru",
					"maxAgeMs": 3600000,
					"maxEntries": 20
				}
			},
			{
				"name": "something2",
				"urls": {
					"/api/checkserverfirst": {
						"match": "prefix"
					}
				},
				"cache": {
					"optimizeFor": "freshness",
					"strategy": "fifo",
					"maxAgeMs": 3600000,
					"maxEntries": 100,
					"networkTimeoutMs": 1000
				}
			}
		]
	}
}